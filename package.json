{
  "name": "daily-trends",
  "version": "1.0.0",
  "description": "Daily Trends Backend",
  "main": "./dist/index.js",
  "scripts": {
    "start:dailyTrends:backend": "NODE_ENV=production node ./dist/apps/dailyTrends/backend/start.js",
    "dev:dailyTrends:backend": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules ./src/apps/dailyTrends/backend/start.ts",
    "test": "TZ=utc ./node_modules/.bin/mocha ./src/**/*/*.test.ts",
    "test:watch": "npm run test -- --watch-extensions ts --watch",
    "test:component": "TZ=utc ./node_modules/.bin/mocha ./__tests__/component/**/*.test.ts",
    "clean": "rm -rf dist",
    "precompile": "npm run clean",
    "compile": "./node_modules/.bin/tsc -p tsconfig.production.json && tsc-alias -p tsconfig.production.json"
  },
  "keywords": [],
  "author": "Pablo Gijon Mora",
  "license": "ISC",
  "engines": {
    "node": ">=20.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/sinon": "^17.0.2",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.1",
    "@types/uuid-validate": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "expect": "^29.7.0",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.1",
    "source-map-support": "^0.5.21",
    "typedi": "^0.10.0",
    "uuid-validate": "^0.0.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
